<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage  xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:PrismBugs.Views"
             x:Class="PrismBugs.Views.MainTabbedPage"
             xmlns:android="clr-namespace:Microsoft.Maui.Controls.PlatformConfiguration.AndroidSpecific;assembly=Microsoft.Maui.Controls"
             android:TabbedPage.ToolbarPlacement="Bottom"
             SelectedTabColor="#0098A5"
             UnselectedTabColor="#666666"
             BarBackgroundColor="White">
    <NavigationPage Title="TabPage 1">
        <x:Arguments>
            <local:TabbedPage1 Title="TabPage1"/>
        </x:Arguments>
    </NavigationPage>
    <NavigationPage Title="TabPage 2">
        <x:Arguments>
            <local:TabbedPage2 Title="TabPage2"/>
        </x:Arguments>
    </NavigationPage>
</TabbedPage>
